            <Font size="24.0" />
         </font>
         <opaqueInsets>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </opaqueInsets>
         <effect>
            <InnerShadow />
         </effect></TextField>
      <PasswordField fx:id="password" layoutX="604.0" layoutY="419.0" prefHeight="63.0" prefWidth="298.0" promptText="password">
