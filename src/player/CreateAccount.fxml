            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </opaqueInsets>
         <effect>
            <InnerShadow />
         </effect>
         <font>
            <Font size="24.0" />
         </font></PasswordField>
      <Button fx:id="register" layoutX="537.0" layoutY="697.0" mnemonicParsing="false" onAction="#register" prefHeight="63.0" prefWidth="134.0" text="Register">
