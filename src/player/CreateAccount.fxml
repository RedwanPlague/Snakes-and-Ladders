<AnchorPane prefHeight="900.0" prefWidth="1400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="player.CreateAccount">
   <children>
      <ImageView fitHeight="901.0" fitWidth="1421.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../MediaFile/Register.jpg" />
